{# @var meta_entity \K3ssen\GeneratorBundle\MetaData\MetaEntityInterface #}
{# @var generate_options \K3ssen\GeneratorBundle\Generator\CrudGenerateOptions #}
{% block usages %}
use {{ meta_entity.fullClassName }};
{% if meta_entity.namespace | replace({'\\Entity': ''}) != generate_options.defaultBundleNamespace %}
use {{ generate_options.defaultBundleNamespace }}\Datatable\AbstractDatatable;
{% endif %}
use Sg\DatatablesBundle\Datatable\Column\Column;
{%- for property in meta_entity.properties if 'date' in property.ormType and break is not defined %}

use Sg\DatatablesBundle\Datatable\Column\DateTimeColumn;
{% set break = true %}
{% endfor %}

{% endblock %}