{# @var meta_entity \K3ssen\GeneratorBundle\MetaData\MetaEntityInterface #}
{% block class_annotations %}
{% if meta_entity.entityAnnotations | length %}
/**
{% for annotation in meta_entity.entityAnnotations %}
 * @{{ annotation.annotationName }}
{%- if annotation.annotationAttributes |length -%}
    (
        {%- for attributeName, value in annotation.annotationAttributes -%}
            {{- loop.first ? '' : ', ' }}
            {{- attributeName }}=
            {%- if value is same as(false) -%}
                false
            {%- elseif value is same as(true) -%}
                true
            {%- elseif value is same as(null) -%}
                null
            {%- elseif value matches '/^\\d+$/' -%}
                {{ value }}
            {%- else -%}
                "{{ value }}"
            {%- endif -%}
        {%- endfor -%}
    )
{% else %}

{% endif %}
{% endfor %}
 */
{% endif %}
{% endblock %}