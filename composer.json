{
    "name": "k3ssen/extended-generator",
    "description": "Example symfony4 project that uses the GeneratorBundle",
    "keywords": ["generator", "crud","entity", "entities", "meta", "metadata", "symfony", "symfony4"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Driessen",
            "email": "kevin3ssen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "symfony/yaml": "~4.0",
        "symfony/config": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "symfony/process": "~4.0",
        "symfony/validator": "~4.0",
        "symfony/orm-pack": "~1.0",
        "symfony/console": "~4.0",
        "symfony/expression-language": "~4.0"
    },
    "suggest": {
        "symfony/security-bundle": "~4.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "sg/datatablesbundle": "^1.0.4"
    },
    "autoload": {
        "psr-4": { "K3ssen\\GeneratorBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
