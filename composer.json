{
    "name": "k3ssen/generator",
    "description": "Generate content for your symfony4 project",
    "keywords": ["generator", "crud","entity", "entities", "meta", "metadata", "symfony", "symfony4"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Driessen",
            "email": "kevin3ssen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "symfony/yaml": "~3.4|~4.0",
        "symfony/config": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0",
        "symfony/process": "~3.4|~4.0",
        "symfony/validator": "~3.4|~4.0",
        "symfony/orm-pack": "~1.0",
        "symfony/console": "~3.4|~4.0",
        "symfony/expression-language": "~3.4|~4.0",
        "symfony/templating": "~3.4|~4.0",
        "symfony/twig-bundle": "~3.4|~4.0",
        "symfony/dotenv": "~3.4|~4.0"
    },
    "suggest": {
        "symfony/security-bundle": "~3.4|~4.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "sg/datatablesbundle": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "K3ssen\\GeneratorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/",
            "/vendor/",
            "/var/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "Test/App/"
        }
    }
}
